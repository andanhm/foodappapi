{
    "restartable": "rs",
    "ignore": [".git", "node_modules", "bower_components", ".sass-cache"],
    "verbose": true,
    "execMap": {
        "js": "node"
    },
    "watch": ["*.*"],
    "env": {
        "NODE_ENV": "development",
        "PORT": 8080
    },
    "ext": "js json",
    "stdin": true,
    "stdout": true
}